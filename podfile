platform :ios, '9.0'
use_frameworks!
inhibit_all_warnings!

def install_pods
  pod 'RealmSwift'
  pod 'SCLAlertView'
end

target 'MemoAppli' do
  install_pods
end

target 'MemoAppliProduction' do
  install_pods
end

target 'MemoAppliTests' do
  install_pods
end

target 'MemoAppliUITests' do
  install_pods
end
